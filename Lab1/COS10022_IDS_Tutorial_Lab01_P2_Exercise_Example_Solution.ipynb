      "source": [
        "# Exercise 1 - Find the sum of multiples of 3 in the given sequence\n",
        "\n",
        "# Initialisation\n",
        "seq = list(range(0,51))\n",
        "target = 3\n",
        "result = 0\n",
        "\n",
        "# Find the answer\n",
        "for i in seq:\n",
        "    if (i % target) == 0:\n",
        "        print(i)\n",
        "        result += i\n",
        "print(\"Final Reslut is:\",result)"
      ],
